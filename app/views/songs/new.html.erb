<h1>Create a Song</h1>

<%= form_for @song  do |f| %>
  <%= f.label :title %>
  <%= f.text_field :title %>
  <br />
  <%= f.label :artist %>
  <%= f.text_field :artist %>
  <br />
  <%= f.label :genre %>
  <%= f.text_field :genre %>
  <br />
  <%= f.label :notes %>
  <%= f.text_field :notes %>
  <br />

  <%= f.submit %>
<% end %>
